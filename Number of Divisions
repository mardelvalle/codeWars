const divisions = (n, divisor) => {
  let count = 0
  if(n < divisor) {
    return count
  }
  while(n > 0) {
    count++
    n = Math.floor(n/divisor)
    if(n < divisor || n === 1) {
      return count
    }
    else if(n === 0) {
      return count
    }
  }
};
