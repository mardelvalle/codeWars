https://www.codewars.com/kata/56f173a35b91399a05000cb7
function findLongest(str) {
  
  var spl = str.split(" ");
  console.log(spl)
  var longest = 0
  
  for (var i = 0; i < spl.length; i++) {
    if (spl[i].length > longest) {
      console.log(spl[i].length)
      longest = spl[i].length
    }
   
  }
   return longest
}
